{
  "name": "web-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "jss": "jss",
    "prestart": "node ./build/prebuild.js",
    "start": "ng serve --base-href=/",
    "prestart:connected": "node ./build/prebuild.js connected",
    "start:connected": "ng serve --base-href=/ --environment=connected",
    "build": "npm run build:client-bundle && npm run build:server-bundle && npm run build:server-wrapper-bundle",
    "prebuild:client-bundle": "node ./build/prebuild.js prod",
    "build:client-bundle": "cross-env-shell ng build --target=production --environment=prod --base-href / --deploy-url $npm_package_config_sitecoreDistPath/browser --output-path=$npm_package_config_buildArtifactsPath/browser",
    "prebuild:server-bundle": "node ./build/prebuild.js server",
    "build:server-bundle": "cross-env-shell ng build --target=production --app=server --environment=server --output-path=$npm_package_config_buildArtifactsPath/server --output-hashing=false",
    "prebuild:server-wrapper-bundle": "node ./build/prebuild.js && node ./build/prebuild.js server",
    "build:server-wrapper-bundle": "cross-env-shell webpack --config webpack.server.config.js --progress --colors --output-path $npm_package_config_buildArtifactsPath"
  },
  "config": {
    "appName": "web-app",
    "sitecoreDistPath": "/dist/web-app",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "buildArtifactsPath": "./dist",
    "routeDataDir": "./data/routes",
    "language": "en"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.10",
    "@angular/router": "^5.2.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@sitecore-jss/sitecore-jss": "^8.0.3",
    "@sitecore-jss/sitecore-jss-angular": "^8.0.3",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@sitecore-jss/sitecore-jss-cli": "^8.0.3",
    "@sitecore-jss/sitecore-jss-dev-tools": "^8.0.3",
    "@sitecore-jss/sitecore-jss-manifest": "^8.0.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "babel-core": "^6.26.0",
    "chalk": "^2.4.0",
    "codelyzer": "^4.0.1",
    "cross-env": "^5.1.4",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
